FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue m_A 0.00065
);

regions
(
    boxToCell
    {
        box (-1 -1 0) (1 1 0.5);
        fieldValues
        (
            volScalarFieldValue m_A 0.065
        );
    }
);



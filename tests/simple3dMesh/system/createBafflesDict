FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      createBafflesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

internalFacesOnly true;

baffles
{
    baffleFaces
    {
        type        faceZone;
        zoneName    f0;

        patches
        {
            master
            {
                //- Master side patch
                name            Membrane;
                type            patch;
            }
            slave
            {
                // Reuse master data
                ${..master}
            }
        }
    }
}



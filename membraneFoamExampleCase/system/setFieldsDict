FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue m_A 0.00065
);

regions
(
    cellToCell
    {
	set region0;
	//insidePoint (0 0 0.02); 
        fieldValues
        (
            volScalarFieldValue m_A 0.065
        );
    }
);


